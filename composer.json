{
    "name": "smelikmartin/simple-shoptet-xml-feed",
    "description": "Simple library for generate XML feed valid for shoptet platform",
    "type": "library",
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/collections": "^2.2",
        "jms/serializer": "^3.30"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleShoptetXmlFeed\\": [
                "src/"
            ]
        }
    },
    "prefer-stable": true
}
